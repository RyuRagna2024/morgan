model User {
    id                     String                   @id @default(uuid())
    username               String                   @unique
    firstName              String
    lastName               String
    displayName            String
    email                  String                   @unique
    passwordHash           String
    Session                Session[]

    createdAt              DateTime                 @default(now())
    updatedAt              DateTime                 @updatedAt

    @@map("users")
}